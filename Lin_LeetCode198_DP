class Solution {
    public int rob(int[] nums) {
        int n = nums.length;
        int[] maxAmount = new int[n];
        maxAmount[0] = nums[0];
        if(n > 1){
        maxAmount[1] = (nums[1] > nums[0]) ? nums[1] : nums[0];    
        }
        for(int i = 2; i < nums.length; i++){
        maxAmount[i] = Math.max(maxAmount[i-1], maxAmount[i-2] + nums[i]);
        }
        return maxAmount[n-1];
    }
}

